{
    "name": "nswdpc/silverstripe-inline-linker",
    "description": "Inline Link field that saves to gorriecoe/link",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "linkfield",
        "link",
        "inline link editing"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "role" : "Developer"
        }
    ],
    "require": {
        "codem/silverstripe-html5-inputs": "^1",
        "fromholdio/silverstripe-externalurlfield" : "^1.1",
        "gorriecoe/silverstripe-link": "*",
        "silverstripe/asset-admin": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "extra": {
        "expose": [
            "client/dist/js",
            "client/static/styles"
        ]
    }
}
