{
    "name": "nswdpc/silverstripe-inline-linker",
    "description": "Inline Link field that saves to gorriecoe/link",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "linkfield",
        "link",
        "inline link editing"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "role" : "Developer"
        }
    ],
    "require": {
        "codem/silverstripe-html5-inputs": "^0.1",
        "fromholdio/silverstripe-externalurlfield" : "^1",
        "gorriecoe/silverstripe-link": "^1.0.0",
        "silverstripe/asset-admin": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.18",
        "symbiote/silverstripe-phpstan": "^4"
    },
    "extra": {
        "expose": [
            "client/dist/js",
            "client/static/styles"
        ]
    }
}
